declare module mod1;


<<< driver program >>>
start
	declare v, w, r :integer;
	get_value(v);
	w:=22;
	declare m:real;
	[r,m] := use module mod1 with parameters v, w;
	print(r);
	print(m);
end

<<module mod1>>
takes input [a:integer, b:integer];
returns [x:integer, abc:real];
start
	declare c:real;
	c:=10.4;
	x:=a+b-10;
	abc:=b/5+c;
end

